<%-include('./partials/head'); %>
  <body>
    <%-include('./partials/navbar'); %>
      <div class="container">
          <h1 class="text-center">
              Decimal or ASCII Code to Text Online
          </h1>
            <div class="form-group">
		<label for="code">Enter Input ASCII Code:</label>
                    <input class="form-control" name="" id="text" placeholder="Enter Code" required/>

            </div>
            <div class="form-group">
                <button id="button" class="btn btn-block btn-danger">
                    Convert to Text 
                </button>
            </div>
	    <div class="form-group">
		<label for="string">Output:</label>
                <input class="form-control" name="" id="code" placeholder="Ascii Code"/>
            </div>
        
        <br><br>
        <%-include('./partials/services');%>
        <%-include('./partials/jumbotron');%>
      </div>
</body>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script>
var result
function ascii(c) {
      return String.fromCharCode(c);
    }
    $("#button").click(function(){
       var text = $("#text").val()
       $("#code").val(ascii(text))
		
})
</script>
</html>