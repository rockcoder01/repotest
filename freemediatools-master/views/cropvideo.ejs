<%-include('./partials/head'); %>
  <body>
    <%-include('./partials/navbar'); %>

      <div class="container">
          <h1 class="text-center">
              Crop or Cut Portion of Video Online
          </h1>
          <form action="/cropvideo" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label for="files">Video:</label>
                <input class="form-control" multiple type="file" name="file" id="" required>
            </div>
            <div class="form-group">
		<label for="starttime">Start Time</label>
                <input class="form-control" type="time" name="start" value="00:00" required>
            </div>
            <div class="form-group">
		<label for="endtime">End Time</label>
                <input class="form-control" type="time" name="end" value="00:10" required>
            </div>
            <div class="form-group">
                <button class="btn btn-block btn-danger">
                    Crop Video
                </button>
            </div>
        </form>
        <br><br>
        <%-include('./partials/services');%>
        <%-include('./partials/jumbotron');%>
      </div>
</body>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</html>